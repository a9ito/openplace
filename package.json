{
	"name": "openplace",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"exec": "concurrently \"caddy run --config ./Caddyfile\" \"npm run dev\"",
		"dev": "tsx watch src",
		"build": "tsc",
		"start": "node dist",
		"prepare": "husky",
		"setup": "tsx scripts/setup.ts",
		"clear-pixels": "tsx scripts/clear-pixels.ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "tsc --noEmit && eslint --ext .js,.ts src/",
		"lint:fix": "eslint --ext .js,.ts src/ --fix",
		"db:push": "prisma db push",
		"db:generate": "prisma generate",
		"db:deploy": "prisma migrate deploy",
		"db:dev": "prisma migrate dev"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.81",
		"@prisma/client": "^6.18.0",
		"@tinyhttp/app": "^3.0.1",
		"@tinyhttp/cookie-parser": "^2.0.6",
		"@tinyhttp/cors": "^2.0.1",
		"@zip.js/zip.js": "^2.8.8",
		"bcryptjs": "^3.0.2",
		"chalk": "^5.6.2",
		"cidr-tools": "^11.0.3",
		"discord.js": "^14.24.0",
		"dotenv": "^17.2.3",
		"jsonwebtoken": "^9.0.2",
		"milliparsec": "^5.1.0",
		"multer": "^2.0.2",
		"obscenity": "^0.4.5",
		"sirv": "^3.0.2",
		"tsx": "^4.20.6",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@tsconfig/node24": "^24.0.1",
		"@tsconfig/strictest": "^2.0.5",
		"@types/cookie-parser": "^1.4.10",
		"@types/inquirer": "^9.0.9",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.9.1",
		"concurrently": "^9.2.1",
		"eslint": "^9.38.0",
		"eslint-config-chariz": "^2.0.0",
		"husky": "^9.1.7",
		"inquirer": "^12.10.0",
		"lint-staged": "^16.2.6",
		"prisma": "^6.18.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	}
}
